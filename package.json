{
  "name": "react-ag-psd-psdtool",
  "type": "commonjs",
  "version": "1.1.7",
  "description": "PSDTool-like React component using ag-psd-psdtool",
  "author": {
    "name": "34j"
  },
  "license": "MIT",
  "homepage": "https://github.com/34j/react-ag-psd-psdtool",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/34j/react-ag-psd-psdtool.git"
  },
  "bugs": {
    "url": "https://github.com/34j/react-ag-psd-psdtool/issues"
  },
  "keywords": [
    "boilerplate",
    "template",
    "typescript",
    "vitest",
    "pre-commit",
    "semantic-release",
    "typedoc"
  ],
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "files": [
    "dist/src/**/*"
  ],
  "scripts": {
    "build:watch": "tsc --watch --project tsconfig.json",
    "preinstall": "npx only-allow pnpm",
    "build": "tsc --project tsconfig.json",
    "lint": "eslint --fix",
    "typedoc": "vite build && typedoc",
    "typedoc:watch": "typedoc --watch",
    "test:watch": "vitest --watch --coverage --coverage.include=src",
    "test": "vitest --coverage --coverage.include=src",
    "watch": "npm-run-all --race --parallel *:watch",
    "update": "npm-check-updates -u",
    "dev": "vite"
  },
  "dependencies": {
    "@rjsf/core": "6.1.2",
    "@rjsf/react-bootstrap": "6.1.2",
    "@rjsf/utils": "6.1.2",
    "@rjsf/validator-ajv8": "6.1.2",
    "ag-psd": "^28.4.1",
    "ag-psd-psdtool": "^1.1.4",
    "bootstrap": "^5.3.8",
    "react": "^19.2.0",
    "react-bootstrap": "^2.10.10",
    "react-code-blocks": "^0.1.6",
    "react-dom": "^19.2.0",
    "react-dropzone": "^14.3.8",
    "react-icons": "^5.5.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "6.3.0",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/git": "10.0.1",
    "@tsconfig/strictest": "2.0.8",
    "@types/node": "24.10.4",
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3",
    "@typescript-eslint/eslint-plugin": "8.50.1",
    "@vitejs/plugin-react": "5.1.2",
    "@vitest/browser": "4.0.16",
    "@vitest/browser-playwright": "4.0.16",
    "@vitest/coverage-v8": "4.0.16",
    "eslint": "9.39.2",
    "npm-run-all2": "8.0.4",
    "playwright": "1.57.0",
    "semantic-release": "25.0.2",
    "typedoc": "0.28.15",
    "typescript": "5.9.3",
    "vite": "7.3.0",
    "vitest": "4.0.16"
  },
  "typedocOptions": {
    "entryPoints": [
      "src"
    ],
    "tsconfig": "tsconfig.json",
    "headings": false,
    "basePath": "./docs",
    "outputs": [
      {
        "name": "html",
        "path": "./docs/docs"
      }
    ]
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/npm",
      [
        "@semantic-release/git",
        {
          "assets": [
            "CHANGELOG.md",
            "README.md",
            "package.json",
            "package-lock.json"
          ]
        }
      ],
      "@semantic-release/github"
    ]
  }
}
